@model ExportImportRegViewModel
@{
    ViewData["Title"] = @Localizer["Apply ExportImport"]; //"반출입 신청";
}
<script>
    window.onload = function (){
        @* console.log("PREV_URL:"+PREV_URL); *@
        PREV_URL = document.getElementById('aList').href + '?culture=@CultureInfo.CurrentCulture';
        @* console.log("PREV_URL:"+PREV_URL); *@
    }
    var currentTabIndex = 1;
    $(function () {
        $("body").on("click", "#btnTab1", function () {
            @* $(this).addClass('on'); *@
            currentTabIndex = 1;
            $('#btnTab1').addClass('on');
