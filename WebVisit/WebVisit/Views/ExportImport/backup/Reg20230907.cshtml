    // add - 자산 
    function saveAddAssetForm(){
        var myform = document.fmAddAsset;
        @* 반출입 신청: mode 
        ExportImportType 0: 자산, 1: 수리, 2: 노트북
        ExportType 0: 외부업체, 1: 공장간이동, 2: 외부업체경유공장간이동, 3: 개인노트북, 4: 대여노트북
        *@
        @* document.getElementById("errormsg1").innerHTML = '@Localizer["Please select the scheduled return date"]'; *@
        
        const selExportImportPurposeType = myform.ExportImportPurposeType;
        selExportImportPurposeType.disabled = false;
        selExportImportPurposeType.removeAttribute("disabled");
        const selImportType = myform.ImportType;
        selImportType.disabled = false;
        selImportType.removeAttribute("disabled");

        if(myform.ExportImportPurposeType.value.trim().length < 1 || myform.ExportImportPurposeType.value.trim() < 0){
            document.getElementById("errormsg1").innerHTML = '@Localizer["Please select the purpose of exportimport"]';
            myform.ExportImportPurposeType.focus();
            return;
        }
        if(myform.ImportType.value.trim().length < 1 || myform.ImportType.value.trim() < 0 ){
            document.getElementById("errormsg1").innerHTML = '@Localizer["Please select the import classify"]';
            myform.ImportType.focus();
            return;
        }
        if(myform.DeliveryMethodType.value.trim().length < 1 || myform.DeliveryMethodType.value.trim() < 0){
            document.getElementById("errormsg1").innerHTML = '@Localizer["Please select the delivery method"]';
            myform.DeliveryMethodType.focus();
            return;
        }
        if(myform.ExportType.value.trim() == 0 && myform.CompanySabun.value.trim().length < 1 ){ // 외부업체 - 외부업체인수자
            document.getElementById("errormsg1").innerHTML = '@Localizer["Please select an outside vendor"]';
            return;
        }
        if(myform.ExportType.value.trim() == 1 && myform.Sabun.value.trim().length < 1 ){ // 공장간이동 - 반입사업장인수자 
            document.getElementById("errormsg1").innerHTML = '@Localizer["Please select the recipient of the import business"]';
            return;
        }
        if(myform.ExportType.value.trim() == 2  && myform.CompanySabun.value.trim().length < 1){ // 외부업체경유공장간이동 - 외부업체인수자
            document.getElementById("errormsg1").innerHTML = '@Localizer["Please select an outside vendor"]';
            return;
        }
        if(myform.ExportType.value.trim() == 2  && myform.Sabun.value.trim().length < 1 ){ // 외부업체경유공장간이동 - 외부업체인수자/반입사업장인수자
            document.getElementById("errormsg1").innerHTML = '@Localizer["Please select the recipient of the import business"]';
            return;
        }
        if(myform.ApprovalSabun.value.trim().length < 1 ){
            document.getElementById("errormsg1").innerHTML = '@Localizer["Please select a approver"]';
            return;
        }
        if(myform.ContactSabun.value.trim().length < 1 ){
            document.getElementById("errormsg1").innerHTML = '@Localizer["Please select a sub contact person"]';
            return;
        }

        //품목 정보 자재이름, 수량, 단위 필수 
        for(var i =0 ;i<myform.length; i++) {
            var el = myform[i];
            // MaterialsName[] Quantity[]
            if (el.name == "MaterialsName[]" && el.value.trim().length < 1) {
                document.getElementById("errormsg1").innerHTML = '@Localizer["Please enter the material name"]';
                el.focus();
                return;
            }
            if (el.name == "Quantity[]") {
                if (el.value.trim().length < 1) {
                    document.getElementById("errormsg1").innerHTML = '@Localizer["Please enter quantity"]';
                    el.focus();
                    return;
                } else if (isNumber(el.value.trim()) == false) {
                    document.getElementById("errormsg1").innerHTML = '@Localizer["Please enter a number for quantity"]';
                    el.focus();
                    return;
                } else if (el.value.trim() < 1) {
                    document.getElementById("errormsg1").innerHTML = '@Localizer["Please enter a value greater than 1 for quantity"]';
                    el.focus();
                    return;
                }
            }
        }
        var con = confirm('@Localizer["Would you like to apply for importexport"]');        
        if(con){
            myform.submit();
        }
    }
    
