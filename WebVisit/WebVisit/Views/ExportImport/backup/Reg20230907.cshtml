    function selectExportType(fmName){
        const myform = eval("document."+fmName+";");
        const exportImportType = myform.ExportImportType.value; //ExportImportType - 0: 자산, 1: 수리, 2: 노트북  
        const selExportImportPurposeType = myform.ExportImportPurposeType;
        const selImportType = myform.ImportType;
        const raExportType = myform.ExportType;
        var strExportType=''; // ExportType - 0: 외부업체, 1: 공장간이동, 2: 외부업체경유공장간이동
        for (i = 0; i < raExportType.length; i++) {
            if (raExportType[i].checked)
                strExportType=raExportType[i].value;
        }
        if(exportImportType == "1"){ //수리 
            //div: dImportAceptor2(반입사업장 인수자)
            if(strExportType == "2"){ // 외부업체경유공장간이동
                $('#dImportAceptor2').css("display", "inline");
            }else{ //외부업체
                $('#dImportAceptor2').css("display", "none");
            }
        }else{ //자산 
            //div: dExternalCompanyAceptor(외부업체 인수자), dImportAceptor(반입사업장 인수자)

            var exportImportPurposeTypeArr = [];
            if(strExportType == "1"){ // 공장간이동
                $('#dExternalCompanyAceptor').css("display", "none");
                $('#dImportAceptor').css("display", "inline");
                // 반입소속사업장(ImportLocation) 선택가능 
                myform.ImportLocation.disabled = true;
                myform.ImportLocation.removeAttribute("disabled");

                // 반출입목적(ExportImportPurposeType) 선택고정 - 5 이체(이동) 
                selExportImportPurposeType.disabled = true;
                selExportImportPurposeType.disabled = "disabled";
                 for (let i=0; i<selExportImportPurposeType.options.length; i++){
                    if(selExportImportPurposeType.options[i].value == 5){
                        selExportImportPurposeType.options[i].disabled = false;
                        selExportImportPurposeType.options[i].selected = true;
                    }else{
                        selExportImportPurposeType.options[i].disabled = true;
                    }
                }

                // 반입구분 선택고정 ImportType: 0 반입요 
                selImportType.disabled = false;
                selImportType.removeAttribute("disabled");
                 for (let i=0; i<selImportType.options.length; i++){
                    if(selImportType.options[i].value == 0){
                        selImportType.options[i].disabled = false;
                        selImportType.options[i].selected = true;
                    }else{
                        selImportType.options[i].disabled = true;
                    }
                }

            }else if(strExportType == "2"){ // 외부업체경유공장간이동
                exportImportPurposeTypeArr = ['1', '3', '4', '7', '8', '10', '12'];
                $('#dExternalCompanyAceptor').css("display", "inline");
                $('#dImportAceptor').css("display", "inline");
                // 반입소속사업장(ImportLocation) 선택가능 
                myform.ImportLocation.disabled = false;
                myform.ImportLocation.removeAttribute("disabled");

                // 반출입목적(ExportImportPurposeType) 부분적 선택가능 - 1: 리클레임, 3: 세정(재생), 4: 교환, 7: 분석, 8: Demo, 10: Warranty, 12: 태블릿
                selExportImportPurposeType.disabled = false;
                selExportImportPurposeType.removeAttribute("disabled");
                for (let i=0; i<selExportImportPurposeType.options.length; i++){                                      
                    if(exportImportPurposeTypeArr.includes(selExportImportPurposeType.options[i].value)){
                        selExportImportPurposeType.options[i].disabled = false;
                    }else{
                        selExportImportPurposeType.options[i].disabled = true;
                    }
                    if(i==0){
                        selExportImportPurposeType.options[i].disabled = false;
                        selExportImportPurposeType.options[i].selected = true;
                    }
                }

                // 반입구분(ImportType): : 선택 불가 ExportImportPurposeType 에 따라 선택 가능한 항목이 정해짐 
                selImportType.options[0].selected = true;
                selImportType.disabled = true;
                selImportType.disabled = "disabled";

            }else{ //외부업체 
                exportImportPurposeTypeArr = ['1', '2', '3', '4', '7', '8', '10', '11', '12'];
                $('#dExternalCompanyAceptor').css("display", "inline");
                $('#dImportAceptor').css("display", "none");
                // 반입소속사업장(ImportLocation) 선택불가 
                myform.ImportLocation.disabled = true;
                myform.ImportLocation.options[0].selected = true;
                myform.ImportLocation.disabled = "disabled";
                
                /* form 보낼때 다시 해제하기
                myform.ImportLocation.disabled = false;
                myform.ImportLocation.removeAttribute("disabled");
                */
                // 반출입목적(ExportImportPurposeType) 부분적 선택가능 - 1: 리클레임, 2: 판매, 3: 세정(재생), 4: 교환, 7: 분석, 8: Demo, 10: Warranty, 11: 반환, 12: 태블릿                
                selExportImportPurposeType.disabled = false;
                selExportImportPurposeType.removeAttribute("disabled");
                 for (let i=0; i<selExportImportPurposeType.options.length; i++){
                    if(exportImportPurposeTypeArr.includes(selExportImportPurposeType.options[i].value)){
                        selExportImportPurposeType.options[i].disabled = false;
                    }else{
                        selExportImportPurposeType.options[i].disabled = true;
                    }
                    if(i==0){
                        selExportImportPurposeType.options[i].disabled = false;
                        selExportImportPurposeType.options[i].selected = true;
                    }
                }

                // 반입구분(ImportType): 선택 불가 ExportImportPurposeType 에 따라 선택 가능한 항목이 정해짐 
                selImportType.options[0].selected = true;
                selImportType.disabled = true;
                selImportType.disabled = "disabled";
            }
        }
    }

