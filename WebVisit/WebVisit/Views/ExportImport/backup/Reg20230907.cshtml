    function goSearchMaterial(obj){
        var myform;
        var j = -1;
        var itemNameObj;
        var itemCodeObj;
        var standardObj;
        var unitObj;
        var prNoObj;
        if (currentTabIndex == 1) {
            myform = document.fmAddAsset;
        } else if(currentTabIndex == 2) {
            myform = document.fmAddRepairForm;
            popSelectMaterial(null, itemNameObj, itemCodeObj, standardObj, unitObj, prNoObj);
        }
        if (myform != null){
            var idx = findIndex(myform, "MaterialsCode[]", obj);
            if (idx > -1) {
                for(var el of myform) {
                    if (el.name == "MaterialsCode[]"){
                        j++;
                    }
                    if(idx == j) {                        
                        if (el.name == "MaterialsCode[]"){
                            itemCodeObj = el;
                        }
                        if (el.name == "MaterialsName[]"){
                            itemNameObj = el;
                        }
                        if (el.name == "Unit[]"){
